<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="iphone14SeventyoneVM" type="com.natashasapplication.app.modules.iphone14seventyone.data.viewmodel.Iphone14SeventyoneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameIPhone14Sev"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylepurple_700_f2">
        <LinearLayout android:id="@+id/linearRowarrowleft"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_8">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_24pxh"
                android:layout_width="@dimen/_32pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            <LinearLayout android:id="@+id/linearColumnlanguage"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{iphone14SeventyoneVM.iphone14SeventyoneModel.txtLanguage}"
                    style="@style/txtMerriweatherSansromanregular20" tools:text="@string/lbl_your_cart"/>
                <TextView android:ellipsize="end" android:gravity="end"
                    android:id="@+id/txtITEMSADDED"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{iphone14SeventyoneVM.iphone14SeventyoneModel.txtITEMSADDED}"
                    style="@style/txtMerriweatherSansromanregular20_3" tools:text="@string/lbl_item_s_added"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnDeliveryWithin2025Mints"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_50pxv"
            android:layout_width="@dimen/_349pxh"
            android:paddingBottom="@dimen/_12pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_12pxv" android:singleLine="true"
            android:text="@string/msg_delivery_within"
            android:textAllCaps="false"
            style="@style/btnGradientRoundedOutline" tools:text="@string/msg_delivery_within"/>
    </FrameLayout>
</layout>
